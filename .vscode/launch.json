{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug C (per-category)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/${relativeFileDirname}/../build/${fileBasenameNoExtension}.exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "preLaunchTask": "build C (per-category)"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/coding_test/Implementation/src",
      "program": "c:/coding_test/Implementation/src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}